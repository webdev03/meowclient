{
  "name": "meowclient",
  "version": "3.1.0",
  "description": "Client for Scratch in JS",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --minify --dts",
    "build:watch": "tsup src/index.ts --format cjs,esm --minify --dts --watch",
    "prepare": "tsup src/index.ts --format cjs,esm --minify --dts",
    "test": "tap --no-coverage --node-arg=--loader=ts-node/esm --node-arg=--experimental-specifier-resolution=node",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@types/tap": "^15.0.8",
    "@types/ws": "^8.5.4",
    "prettier": "^2.8.8",
    "tap": "^16.3.4",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "node-html-parser": "^5.4.2",
    "ws": "^8.13.0"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "engines": {
    "node": ">13"
  },
  "engineStrict": true,
  "sideEffects": false
}
